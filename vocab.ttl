@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix desgn: <https://jardindesconnaissances.univ-paris8.fr/onto/desgn#> .
@prefix teach: <http://linkedscience.org/teach/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.

<https://jardindesconnaissances.univ-paris8.fr/onto/desgn> a owl:Ontology ;
    dcterms:title "Design" .

desgn:UI a rdfs:Class ;
    rdfs:label "UI Design" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:UX a rdfs:Class ;
    rdfs:label "UX Design" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:Interactif a rdfs:Class ;
    rdfs:label "Design Interactif" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:Animation a rdfs:Class ;
    rdfs:label "Design d'animation" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:Jeu a rdfs:Class ;
    rdfs:label "Design de jeux vidéo" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:VR a rdfs:Class ;
    rdfs:label "Design de réalité virtuelle" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:AR a rdfs:Class ;
    rdfs:label "Design de réalité augmentée" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:VoiceUI a rdfs:Class ;
    rdfs:label "Design d'interfaces vocales" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:hasUI a rdf:Property ;
    rdfs:label "Facilite d'utilisation" ;
    rdfs:comment "Relation avec l'UI" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:hasUX a rdf:Property ;
    rdfs:label "Utilité" ;
    rdfs:comment "Relation avec l'UX" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

desgn:hasInteractif a rdf:Property ;
    rdfs:label "Réactivité" ;
    rdfs:comment "Relation avec l'Interactif" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

